<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_assignments"
    app:startDestination="@id/assginmentSFragment">

    <fragment
        android:id="@+id/assginmentSFragment"
        android:name="com.one.feature.assignment.student.AssginmentSFragment"
        android:label="fragment_assginment_s"
        tools:layout="@layout/fragment_assginment_s" >
        <action
            android:id="@+id/action_assginmentSFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
        <action
            android:id="@+id/action_assginmentSFragment_to_fileUploadFragment"
            app:destination="@id/fileUploadFragment" />
    </fragment>
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.one.feature.assignment.student.feedback.FeedbackFragment"
        android:label="fragment_feedback"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/fileUploadFragment"
        android:name="com.one.feature.assignment.student.upload.FileUploadFragment"
        android:label="fragment_file_upload"
        tools:layout="@layout/fragment_file_upload" />
</navigation>